    "d3-force": "^3.0.0",
    "d3-geo": "^3.1.1",
    "d3-geo-polygon": "^1.12.1",
    "d3-geo-projection": "^4.0.0",
    "d3-interpolate": "^3.0.1",
    "d3-quadtree": "^3.0.1",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-tile": "^1.0.0",
    "d3-transition": "^3.0.1",
    "d3-zoom": "^3.0.0",
    "dexie": "^4.0.8",
    "dicopal": "^0.8.1",
    "dompurify": "^3.1.6",
    "font-gis": "^1.0.6",
    "gdal3.js": "^2.8.1",
    "geos-wasm": "^2.0.0",
    "go-cart-wasm": "^0.3.0",
    "interactjs": "^1.10.27",
    "jszip": "^3.10.1",
    "main-thread-scheduling": "^14.3.0",
    "mproj": "^0.0.39",
    "nouislider": "^15.8.1",
